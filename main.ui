    border-radius: 0.5rem;
    padding: 0.5rem 1rem;
}

QLineEdit::placeholder {
    color: #4B5563;
    opacity: 1;
}

QLineEdit:disabled, QLineEdit[readonly] {
    background-color: #E5E7EB;
    opacity: 1;
}

